{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigin": "http://localhost:4200",
  // used to make part of the random path of the image, so that the there wouldn't be two images with the same path
  "ImagePathCharSet": "abcdefghijklomnopqrst",

  // is the format in which the image is saved in the server
  "ImageEncodingFormat": ".jpg",
  "ImagesRelativePath": "Resources/Images",
  "Roles": {
    "Admin": "Admin",
    "Consumer": "Consumer",
    "Deliverer": "Deliverer"
  },
  "ValidIssuer": "http://localhost:5897",
  "TokenValidityMinutes": 10
}
